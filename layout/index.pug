h1 MOUNTAINBOOOY WANDERING
each post in page.posts.data
  li
    = post.date
    h3
      a(href=url_for(post.path))
        = post.title
    each tag in post.tags.data
      a(href=url_for(tag.path))
        = tag.name + ','
    each category in post.categories.data
      = category.name + ','
          

if page.prev_link
  a(href=url_for(page.prev_link)) Prev
strong= page.current
if page.next_link
  a(href=url_for(page.next_link)) Next

